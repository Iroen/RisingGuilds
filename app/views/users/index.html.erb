<table>
	<thead>
	<tr>
		<th>Login</th>
		<th>Last Login</th>
		<th></th>
	</tr>
	</thead>
	<tbody>
		<tr>
			<th colspan="3">Leaders</th>
		</tr>
		<%= render :partial => 'user', :collection => @guild.leaders, :as => :user, :locals => {:guild => @guild}%>
	</tbody>
	<tbody>
		<tr>
			<th colspan="3">Officers</th>
		</tr>
		<%= render :partial => 'user', :collection => @guild.officers, :as => :user, :locals => {:guild => @guild}%>
	</tbody>
	<tbody>
		<tr>
			<th colspan="3">Raidleaders</th>
		</tr>
		<%= render :partial => 'user', :collection => @guild.raidleaders, :as => :user, :locals => {:guild => @guild}%>
	</tbody>
	<tbody>
		<tr>
			<th colspan="3">Members</th>
		</tr>
		<%= render :partial => 'user', :collection => @guild.members, :as => :user, :locals => {:guild => @guild}%>
	</tbody>
</table>

<%= yield :tooltip %>